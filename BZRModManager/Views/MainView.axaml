  <SplitView IsPaneOpen="True"
             DisplayMode="CompactInline"
             OpenPaneLength="375">
    <SplitView.Pane>
      <DockPanel Margin="10">
          <TextBlock DockPanel.Dock="Top"
                     Text="BZR Mod Manager"
                     FontSize="24"
                     FontWeight="Bold"
                     Margin="0 0 0 10"/>
        <ScrollViewer AllowAutoHide="True">
          <StackPanel Margin="0 0 10 0">
            <TextBlock Text="LAUNCH"/>
            <controls:MainNavButton Text="Battlezone 98 Redux"
                                    IconData="{StaticResource icon_steam}"
                                    ImageSource="/Assets/bz98r.ico"/>
            <controls:MainNavButton Text="Battlezone 98 Redux"
                                    IconData="{StaticResource icon_gog}"
                                    ImageSource="/Assets/bz98r.ico"/>
            <controls:MainNavButton Text="Battlezone Combat Commander"
                                    IconData="{StaticResource icon_steam}"
                                    ImageSource="/Assets/bzcc.ico"/>
            <controls:MainNavButton Text="Battlezone Combat Commander"
                                    IconData="{StaticResource icon_gog}"
                                    ImageSource="/Assets/bzcc.ico"/>
            <controls:MainNavButton Text="BZCC (Force Stock)"
                                    IconData="{StaticResource icon_steam}"
                                    ImageSource="/Assets/bzcc.ico"/>
            <controls:MainNavButton Text="BZCC (Force Stock)"
                                    IconData="{StaticResource icon_gog}"
                                    ImageSource="/Assets/bzcc.ico"/>
            <TextBlock Margin="0 10 0 0"
                       Text="MODS" />

            <controls:MainNavButton Text="Manage Mods"
                                    IconData="{StaticResource icon_managemods}"/>
            <controls:MainNavButton Text="Get Mods"
                                    IconData="{StaticResource icon_getmods}"/>
            <controls:MainNavButton Text="Multiplayer"
                                    IconData="{StaticResource icon_multiplayer}"/>
            <controls:MainNavButton Text="Chat"
                                    IconData="{StaticResource icon_chat}"/>

            <TextBlock Margin="0 10 0 0"
                       Text="OTHER"/>

            <controls:MainNavButton Text="Settings"
                                    IconData="{StaticResource icon_settings}"/>
            <controls:MainNavButton Text="Tasks"
                                    IconData="{StaticResource icon_tasks}"/>
            <controls:MainNavButton Text="SteamCMD"
                                    IconData="{StaticResource icon_steamcmd}"
                                    Command="{Binding ChangeContentCommand}"/>
            <controls:MainNavButton Text="About"
                                    IconData="{StaticResource icon_about}"/>

            <TextBlock Margin="0 10 0 0"
                       Text="OTHER X"/>

            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
            <controls:MainNavButton Text="SCROLL DUMMY"/>
          </StackPanel>
        </ScrollViewer>
      </DockPanel>
    </SplitView.Pane>
    <SplitView.Content>
      <Panel>
        <Image DockPanel.Dock="Top"
               Source="/Assets/modmanager.ico"
               Height="256"
               Width="256"
               Opacity="0.25"
               HorizontalAlignment="Right"
               VerticalAlignment="Bottom"
               Margin="20"/>
        <!--
        <TextBlock Text="{Binding Greeting}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        -->
        <!--
        <Panel Margin="0 30 0 0" Background="Red"></Panel>
        -->
        <ContentPresenter Content="{Binding ContentViewModel}"/>
        <!--
        <Panel x:Name="Content"></Panel>
        -->
      </Panel>
    </SplitView.Content>
  </SplitView>
</UserControl>
